#!/usr/bin/env pwsh

function Get-Min-Time([int64]$distance, [int64]$time) {
  for($t=1;$t -lt $time;$t++){
    if($t*($time-$t) -gt $distance){
      return $t
    }
  }
}

function Get-Max-Time([int64]$distance, [int64]$time) {
  for($t=$time;$t -gt 0;$t--){
    if($t*($time-$t) -gt $distance){
      return $t
    }
  }
}

